{
  "name": "importarr",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "check": "bunx tsc --noEmit",
    "dev": "bun run --watch --hot ./src/app",
    "download:tmdb": "curl https://developer.themoviedb.org/openapi/tmdb-api.json | jq -f ./clean-tmdb-schema.jq > tmdb-api.json",
    "generate:tmdb": "openapi --name Tmdb --input ./tmdb-api.json --output ./src/generated/tmdb --useOptions --client axios",
    "generate:prisma": "prisma generate"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/bun": "latest",
    "@types/byte-size": "^8.1.2",
    "lefthook": "^1.13.6",
    "openapi-typescript-codegen": "^0.29.0",
    "prisma": "^6.19.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@ctrl/video-filename-parser": "^5.4.1",
    "@hono/node-server": "^1.19.7",
    "@hono/zod-validator": "^0.7.5",
    "@prisma/client": "^6.19.0",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "bulma": "^1.0.4",
    "byte-size": "^9.0.1",
    "classnames": "^2.5.1",
    "croner": "^9.1.0",
    "form-data": "^4.0.5",
    "hono": "^4.10.8",
    "hono-sessions": "^0.8.0",
    "zod": "^4.1.13"
  }
}
